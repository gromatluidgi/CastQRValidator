<Page x:Class="CastQRValidator.Views.Pages.EnginesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CastQRValidator.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EnginesPage">

    <DockPanel Margin="16" LastChildFill="False">
        <StackPanel
            HorizontalAlignment="Left"
            DockPanel.Dock="Top"
            Orientation="Vertical">
            <TextBlock Style="{StaticResource MaterialDesignHeadline3TextBlock}" Text="Engines" />
            <TextBlock Style="{StaticResource MaterialDesignSubtitle1TextBlock}" Text="Manage tools used to run the plans." />
        </StackPanel>
    </DockPanel>
</Page>
